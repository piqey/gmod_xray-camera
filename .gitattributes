# Auto detect text files and perform LF normalization
* text=auto

### CodeRage Garry's Mod LFS Trackers
## Compiled (loaded) assets
# Textures
*.vtf filter=lfs diff=lfs merge=lfs -text	# Valve Texture File
*.png filter=lfs diff=lfs merge=lfs -text
*.tga filter=lfs diff=lfs merge=lfs -text	# Targa
*.jpg filter=lfs diff=lfs merge=lfs -text
# Models
*.mdl filter=lfs diff=lfs merge=lfs -text
*.vtx filter=lfs diff=lfs merge=lfs -text
*.phy filter=lfs diff=lfs merge=lfs -text
*.vvd filter=lfs diff=lfs merge=lfs -text
# Sound
*.wav filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
# Misc.
*.pcf filter=lfs diff=lfs merge=lfs -text	# Particle systems
*.ttf filter=lfs diff=lfs merge=lfs -text	# True-Type Font
## Raw (unloaded) assets
# Adobe project files
*.psd filter=lfs diff=lfs merge=lfs -text	# Photoshop
*.ai filter=lfs diff=lfs merge=lfs -text	# Illustrator
# Source engine
*.qc filter=lfs diff=lfs merge=lfs -text	# Model compilation data
*.qci filter=lfs diff=lfs merge=lfs -text	# Model compilation data includes
*.smd filter=lfs diff=lfs merge=lfs -text	# Studiomdl Data
*.dmx filter=lfs diff=lfs merge=lfs -text	# Data Model eXchange (substitute for studiomdl/SMD)
*.fbx filter=lfs diff=lfs merge=lfs -text	# Popular model format
*.obj filter=lfs diff=lfs merge=lfs -text	# "
*.mat filter=lfs diff=lfs merge=lfs -text
*.meta filter=lfs diff=lfs merge=lfs -text
*.mtl filter=lfs diff=lfs merge=lfs -text	# "
*.blend filter=lfs diff=lfs merge=lfs -text
*.blend1 filter=lfs diff=lfs merge=lfs -text
*.ai filter=lfs diff=lfs merge=lfs -text
*.psd filter=lfs diff=lfs merge=lfs -text

